{
  "name": "solidity-language-server",
  "publisher": "jeason",
  "version": "1.0.0",
  "description": "Language server protocol implement for Solidity.",
  "scripts": {
    "vscodeweb": "vscode-test-web --extensionDevelopmentPath=. --open-devtools --quality=stable ./contracts",
    "prepublishOnly": "pnpm run build",
    "dev": "pnpm run \"/^dev:.+/\"",
    "dev:web": "webpack --config webpack.web.config.js --watch",
    "dev:node": "webpack --config webpack.node.config.js --watch",
    "build": "pnpm run \"/^build:.+/\"",
    "build:web": "webpack --config webpack.web.config.js",
    "build:node": "webpack --config webpack.node.config.js",
    "lint": "pnpm run \"/^lint:.+/\"",
    "lint:eslint": "eslint --quiet --fix --ext .ts .",
    "lint:prettier": "prettier **/*.ts --log-level warn --write",
    "husky:prepare": "husky install",
    "husky:pre-commit": "lint-staged"
  },
  "author": "Jeason <me@cowpoke.cc>",
  "repository": "git@github.com/jeasonstudio/solidity-language-server.git",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/jeasonstudio/solidity-language-server#readme",
  "dependencies": {
    "@remax-ide/solidity-parser": "^0.16.0",
    "debug": "^4.3.4",
    "lodash-es": "^4.17.21",
    "solidity-antlr4": "^2.4.1",
    "type-fest": "^4.9.0",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11",
    "vscode-uri": "^3.0.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.7",
    "@types/vscode": "^1.85.0",
    "@umijs/fabric": "^4.0.1",
    "@vscode/test-web": "^0.0.50",
    "buffer": "^6.0.3",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.1.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "~5.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "lint-staged": {
    "*.ts": "eslint --quiet --fix",
    "*.{ts}": "prettier --loglevel warn --write"
  },
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/node/extension.js",
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "solidity",
        "aliases": [
          "Solidity",
          "solidity"
        ],
        "extensions": [
          ".sol"
        ],
        "configuration": "./scripts/solidity.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "solidity",
        "scopeName": "source.solidity",
        "path": "./scripts/solidity.syntaxes.json"
      }
    ]
  }
}
